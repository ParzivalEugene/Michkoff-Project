{
  "discord.lowerDetailsEditing": "Michkoff Project",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "prettier": "explicit"
  },
  "editor.tabSize": 2,
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "files.exclude": {
    ".next": true,
    "node_modules": true
  },
  "exportall.config.folderListener": ["/renderers"]
}
